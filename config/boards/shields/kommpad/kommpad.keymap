#include <behaviors.dtsi>
#include <dt-bindings/zmk/bt.h>
#include <dt-bindings/zmk/keys.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        NUMS {
            bindings = <
&kp N1  &kp N2  &kp N3
&kp N4  &kp N5  &mo 1
            >;
        };

        BT {
            bindings = <
&trans        &trans        &bootloader
&bt BT_SEL 0  &bt BT_SEL 1  &trans
            >;
        };
    };
};